{
    "name": "mediagone/types-enums-doctrine",
    "type": "library",
    "description": "Provides Doctrine types for mediagone/types-enums package.",
    "keywords": ["doctrine", "enum", "enums"],
    "license": "MIT",
    "authors": [
        {"name": "Bruce Suire"}
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mediagone\\Doctrine\\Types\\Enums\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Mediagone\\Doctrine\\Types\\Enums\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "doctrine/dbal": "^2.7|^3.0",
        "mediagone/types-enums": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "tests": "phpunit -c phpunit.xml.dist --testsuite Unit_Tests"
    }
  }
